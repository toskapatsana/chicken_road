# Zero-Trace Policy: iOS .gitignore

####################################
# Xcode user data (CRITICAL)
####################################
xcuserdata/
**/xcuserdata/
*.xcuserstate
*.xcuserdatad/

####################################
# Derived data / Build artifacts
####################################
DerivedData/
**/DerivedData/
build/
**/build/
*.ipa
*.app
*.appex
*.dSYM
*.dSYM.zip
*.xcarchive
*.xctestrun

####################################
# Signing / Provisioning (CRITICAL)
####################################
*.mobileprovision
*.p8
*.p12
*.cer
*.pem
/Certificates/
/Provisioning/

####################################
# CocoaPods
####################################
Pods/
**/Pods/
Podfile.lock
.symlinks/

####################################
